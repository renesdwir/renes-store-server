<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Name</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i< category.length; i++){%>
            <tr>
                <td>
                    <%=i+1%>
                </td>
                <td>
                    <%=category[i].name%>
                </td>
                <td>
                    <form action="/category/delete/<%= category[i]._id %>?_method=DELETE" method="POST">
                        <a href="/category/edit/<%= category[i]._id %>" class="btn btn-sm btn-warning mr-1"><i
                                class="fas fa-pencil-alt mr-2"></i>Ubah</a>
                        <button class="btn btn-danger btn-sm"><i class="fas fa-trash mr-2"></i>Hapus</button>
                    </form>
                </td>
            </tr>
            <%}%>
    </tbody>
</table>