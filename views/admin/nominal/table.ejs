<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No.</th>
            <th>Coin Name</th>
            <th>Coin Quantity</th>
            <th>Coin Price</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i< nominal.length; i++){%>
            <tr>
                <td>
                    <%=i+1%>
                </td>
                <td>
                    <%=nominal[i].coinName%>
                </td>
                <td>
                    <%=nominal[i].coinQuantity%>
                </td>
                <td>
                    <%=nominal[i].price%>
                </td>
                <td>
                    <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
                        <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn sm btn-info btn-sm mr-1"><i
                                class="fas fa-pencil-alt mr-2"></i>Ubah</a>
                        <button class="btn btn-danger btn-sm"><i class="fas fa-trash mr-2"></i>Hapus</button>
                    </form>
                </td>
            </tr>
            <%}%>
    </tbody>
</table>